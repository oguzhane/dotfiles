{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "ubuntu": "<#EF7D00>\uf31b</>",
            "windows": "<#028AF2>\ue62a</>"
          },
          "template": "{{.Icon}}",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .UserName }} ",
          "foreground": "#ffafd2",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "home_icon": "\ue5ff ~",
            "style": "full"
          },
          "template": "{{ .Path }} ",
          "foreground": "#EF7D00",
          "type": "path",
          "style": "plain",
          "exclude_folders": [
            "/mnt/c",
            "/mnt/c/.*"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "mapped_locations": {
              "/mnt/c/Users/oguzh": "\ue5ff ~",
              "/mnt/c/Users/oguzhanergin": "\ue5ff ~"
            },
            "style": "full"
          },
          "template": "{{ .Path }} ",
          "foreground": "#3b78ff",
          "type": "path",
          "style": "plain",
          "include_folders": [
            "/mnt/c",
            "/mnt/c/.*"
          ]
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none",
            "fetch_stash_count": true
          },
          "template": "<#ffffff>on</> {{ .HEAD }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "[\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}] ",
          "foreground": "#906cff",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "[\ufcd1 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}] ",
          "foreground": "#7FD5EA",
          "type": "go",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "[\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}] ",
          "foreground": "#76b367",
          "type": "node",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "‚ùØ",
          "foreground": "#FFD54F",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
